PNT VR, VV, V(20)
INT I, J : FLT S

B(4).1=0 : B(4).2=1 : WAIT 500
B(4).2=0 : B(4).0=0

SPD = 8000
MPTP P1 : CALL PICK
S=20.0 : VV=P2 : MPTP VV

FOR J=1 TO 12
	IF J==1 : CALL WRITE01 : ENDI
	IF J==2 : CALL WRITE02 : ENDI
	IF J==3 : CALL WRITE03 : ENDI
	IF J==4 : CALL WRITE04 : ENDI
	IF J==5 : CALL WRITE05 : ENDI
	IF J==6 
		VV=P2 : VV.2=VV.2+2.4*S : MPTP VV
		CALL WRITE06
	ENDI
	IF J==7 : CALL WRITE07 : ENDI
	IF J==8 : CALL WRITE08 : ENDI
	IF J==9 : CALL WRITE09 : ENDI
	IF J==10 : CALL WRITE00 : ENDI
	IF J==11 : CALL WRITEEQ : ENDI
	IF J==12 : CALL WRITEDO : ENDI
	VV.1=VV.1+1.5*S : MPTP VV
ENDF
MPTP P1 : CALL PLACE

MEND


SPGM WRITE01
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).1 = VR.1 + 0.5*S : V(2)=V(1)
	V(2).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	MPTP V(2)
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE02
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).2=VR.2+0.3*S
	V(2).1=VR.1+0.3*S
	V(3).1=VR.1+0.7*S
	V(4).1=VR.1+1.0*S : V(4).2=VR.2+0.3*S
	V(5).2=VR.2+1.6*S
	V(6).1=VR.1+1.0*S : V(6).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 6 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE03
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(2).1=VR.1+0.7*S
	V(3).1=VR.1+1.0*S
	V(3).2=VR.2+0.3*S
	V(4).1=VR.1+1.0*S
	V(4).2=VR.2+0.5*S
	V(5).1=VR.1+0.7*S
	V(5).2=VR.2+0.8*S
	V(6).2=VR.2+0.8*S
	V(7).1=V(5).1
	V(7).2=V(5).2
	V(8).1=VR.1+1.0*S
	V(8).2=VR.2+1.1*S
	V(9).1=VR.1+1.0*S
	V(9).2=VR.2+1.3*S
	V(10).1=VR.1+0.7*S
	V(10).2=VR.2+1.6*S
	V(11).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 11 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE04
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).1=VR.1+1.0*S
	V(1).2=VR.2+0.8*S
	V(2).2=VR.2+0.8*S
	V(3).1=VR.1+0.5*S
	V(4).1=VR.1+0.5*S
	V(4).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 4 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE05
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).1=VR.1+1.0*S
	V(3).2=VR.2+0.8*S
	V(4).1=VR.1+0.7*S
	V(4).2=VR.2+0.8*S
	V(5).1=VR.1+1.0*S
	V(5).2=VR.2+1.1*S
	V(6).1=VR.1+1.0*S
	V(6).2=VR.2+1.3*S
	V(7).1=VR.1+0.7*S
	V(7).2=VR.2+1.6*S
	V(8).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 8 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE06
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).1=VR.1+1.0*S
	V(3).2=VR.2+1.6*S
	V(4).1=VR.1+1.0*S
	V(4).2=VR.2+1.6*S
	V(5).1=VR.1+1.0*S
	V(5).2=VR.2+0.8*S
	V(6).2=VR.2+0.8*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 6 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE07
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).2=VR.2+0.8*S
	V(3).1=VR.1+1.0*S
	V(4).1=VR.2+1.0*S
	V(4).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 4 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITE08
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).1=VR.1+1.0*S
	V(1).2=VR.2+0.8*S
	V(2).2=VR.2+0.8*S
	V(4).1=VR.1+1.0*S
	V(5).1=VR.1+1.0*S
	V(5).2=VR.2+1.6*S
	V(6).2=VR.2+1.6*S
	V(7).1=V(2).1
	V(7).2=V(2).2
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 7 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND


SPGM WRITE09
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).1=VR.1+1.0*S
	V(1).2=VR.2+0.8*S
	V(2).2=VR.2+0.8*S
	V(4).1=VR.1+1.0*S
	V(5).1=VR.1+1.0*S
	V(5).2=VR.2+1.6*S
	V(6).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 6 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND


SPGM WRITE00
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(2).1=VR.1+1.0*S
	V(3).2=VR.2+1.6*S
	V(3).1=VR.1+1.0*S
	V(4).2=VR.2+1.6*S
	V(5).1=V(1).1
	V(5).2=V(1).2
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 5 : MPTP V(I) : ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITEEQ
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).2=VR.2+0.5*S
	V(2).1=VR.1+1.0*S
	V(2).2=VR.2+0.5*S
	V(3).2=VR.2+1.1*S
	V(4).1=VR.1+1.0*S
	V(4).2=VR.2+1.1*S
	MPTP V(1)
	CALL SOL1
	FOR I=2 TO 4 
		IF I==3
			CALL SOL2
			MPTP V(I)
			CALL SOL1
		ELSE
			MPTP V(I)
		ENDI
	ENDF
	CALL SOL2
	MPTP VR
SEND

SPGM WRITEDO
	VR=GPNT(512)
	FOR I=1 TO 20 : V(I)=VR : ENDF
	V(1).2=VR.2+1.6*S
	MPTP V(1)
	CALL SOL1
	CALL SOL2
	MPTP VR
SEND

SPGM SOL1
	B(4).1=1 : WAIT 500
	B(4).1=0 : WAIT 500 : STBY (B(0).0==1)
SEND

SPGM SOL2
	B(4).2=1 : WAIT 500
	B(4).2=0 : WAIT 500 : STBY (B(0).1==1)
SEND

SPGM PICK
	CALL SOL1
	B(4).0=1 : WAIT 500 : STBY (B(0).3==1)
	CALL SOL2
SEND

SPGM PLACE
	CALL SOL1
	B(4).0=0 : WAIT 500 : STBY (B(0).2==1)
	CALL SOL2
SEND

